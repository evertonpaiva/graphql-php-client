{
    "name": "ufvjm/graphql-client",
    "description": "Cliente GraphQL para realizar requisições para a API GraphQL da UFVJM.",
    "keywords": [
        "graphql",
        "client",
        "ufvjm"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Divisão de Desenvolvimento de Sistemas",
            "email": "devsis@ufvjm.edu.br"
        },
        {
            "name": "Éverton de Oliveira Paiva",
            "email": "everton.paiva@ufvjm.edu.br"
        }
    ],
    "require": {
        "php": ">=7.1",
        "gmostafa/php-graphql-client": "^1.4",
        "firebase/php-jwt": "5.*"
    },
    "require-dev": {
        "phpmd/phpmd" : "@stable",
        "phpunit/phpunit": ">=8 <=9",
        "sensiolabs/security-checker": "^6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "GraphqlClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GraphqlClient\\Tests\\": "tests/"
        }
    },
    "version": "0.0.3"
}
