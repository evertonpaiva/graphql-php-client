{
    "name": "micro/graphql-client",
    "description": "Cliente GraphQL para realizar requisições para a API GraphQL da UFVJM.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Divisão de Desenvolvimento de Sistemas",
            "email": "devsis@ufvjm.edu.br"
        }
    ],
    "require": {
        "php": ">=7.1",
        "gmostafa/php-graphql-client": "^1.4",
        "firebase/php-jwt": "5.*"
    },
    "require-dev": {
        "phpmd/phpmd" : "@stable",
        "phpunit/phpunit": ">=4.8 <=9",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "GraphqlClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GraphqlClient\\Tests\\": "tests/"
        }
    },
    "version": "0.0.1"
}
